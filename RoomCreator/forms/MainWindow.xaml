<Window x:Class="RoomCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="657" Width="800">
    <Grid x:Name="MainGrid" HorizontalAlignment="Left" Height="609" Margin="0,10,0,0" VerticalAlignment="Top" Width="790">
        <Canvas x:Name="MainCanvas" HorizontalAlignment="Left" Height="565" Margin="0,10,0,0" VerticalAlignment="Top" Width="790" MouseUp="AddNewNode" Background="White" MouseLeftButtonUp="MouseLeftClick" MouseRightButtonUp="MouseRightClick">
        </Canvas>
        <Button x:Name="btnClear" Content="Clear" Width="75" Click="btnClear_Click" Margin="10,584,705,3"/>
        <Button x:Name="btnRestart" Content="Restart" HorizontalAlignment="Left" Margin="90,584,0,0" VerticalAlignment="Top" Width="75" Click="btnRestart_Click" Height="22"/>
        <Button x:Name="btnExport" Content="Export" HorizontalAlignment="Left" Margin="170,584,0,0" VerticalAlignment="Top" Width="75" Click="btnExport_Click"/>
        <Button x:Name="btnImport" Content="Import" HorizontalAlignment="Left" Margin="250,584,0,0" VerticalAlignment="Top" Width="74" Click="btnImport_Click"/>
    </Grid>

</Window>
